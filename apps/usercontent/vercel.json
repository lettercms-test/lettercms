{
  "headers": [
    {
      "source": "/",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://lettercms-dashboard-main.vercel.app"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api" 
    }
  ]
}

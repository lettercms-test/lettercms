{
  "name": "lettercms",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "deploy": "turbo run deploy",
    "dev": "turbo run dev --parallel --filter=dashboard --filter=client --filter=api --filter=cdn",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@lettercms/eslint-config": "*",
    "express": "^4.18.1",
    "dotenv": "^16.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.7.1",
    "turbo": "^1.4.6",
    "vercel": "^28.4.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
